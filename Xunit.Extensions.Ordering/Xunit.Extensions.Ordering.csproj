<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <AssemblyVersion>0.9.0.0</AssemblyVersion>
    <FileVersion>0.9.0.0</FileVersion>
    <PackageProjectUrl>https://github.com/tomaszeman/Xunit.Extensions.Ordering</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Xunit Ordered Tests Priority Core .Net</PackageTags>
    <Authors>Tomáš Zeman</Authors>
    <Version>0.9.0</Version>
    <Company>Tomáš Zeman</Company>
    <Description>Ordered testing with Xunit (Integration testing)</Description>
    <RepositoryUrl>https://github.com/tomaszeman/Xunit.Extensions.Ordering</RepositoryUrl>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <SignAssembly>false</SignAssembly>
    <PackageIconUrl>https://github.com/tomaszeman/Xunit.Extensions.Ordering/blob/master/logo.png</PackageIconUrl>
    <Copyright>@2019</Copyright>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageReleaseNotes>Initial commit</PackageReleaseNotes>
    <NeutralLanguage />
    <DelaySign>true</DelaySign>
    <Product>Xunit.Extensions.Ordering</Product>
    <PackageId>Xunit.Extensions.Ordering</PackageId>
	  <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
	  <IsPackable>true</IsPackable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="xunit" Version="2.4.1" />
  </ItemGroup>

</Project>
